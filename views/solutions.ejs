
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="">
<meta name="author" content="">
<title>HealthCheck</title>
<!-- Bootstrap Core CSS -->
<link href="/assets/css/bootstrap.min.css" rel="stylesheet">
<!-- MetisMenu CSS -->
<link href="/assets/css/metisMenu.min.css" rel="stylesheet">
<!-- Timeline CSS -->
<link href="/assets/css/timeline.css" rel="stylesheet">
<!-- Custom CSS -->
<link href="/assets/css/sb-admin-2.css" rel="stylesheet">
<!-- Morris Charts CSS -->
<link href="/assets/css/plugins/morris.css" rel="stylesheet">
<!-- Custom Fonts -->
<link href="/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet">
</head>
<body>
	<div id="wrapper">
		<%- include nav.ejs %>
		<div id="page-wrapper">
			<div class="container-fluid">

                <div class="row">
						<div class="col-lg-12">
							<h1 class="page-header">Solutions</h1>
						</div>
				</div>

				<div class="row">
				   <div class="col-lg-12">
					<div class="form-group">
						<form action="/services" id="searchForm" method="get">
						<div class="row">
						   <div class="col-lg-2">Problem type:</div>
						    <div class="col-lg-6">
						    	<select class="form-control" name="problemType"  >
							        <option value=""> problem type</option>
							   </select>

						    </div>
						</div>	

                        <br />
						<div class="row">
						   <div class="col-lg-2">Erroe message:</div>
						    <div class="col-lg-8">
						    	<textarea name="errorMessage"  value="" class="form-control" row='3' placeholder="error message"> </textarea>
						    </div>
						    <div class="col-lg-2">
						    	<input name="pageNum" id="pageNum" type="hidden" value=""> 
							    <input id="totalPages" type="hidden">
							   <button type="submit" class="btn btn-primary">Search</button>
						    </div>
						</div>
							
							 
							 
						    
						</form>
					</div>
					</div>
				</div>


				<div class="row">
						<div class="col-lg-12">
							
							<div class="table-responsive">
								<table class="table table-bordered table-hover table-striped">
									<thead>
										<tr>
											<th>Problem type</th>
											<th>Error message</th>
											<th>Description</th>
									
										</tr>
									</thead>
									<tbody>
										  
										  <% if(typeof solutions!='undefined') {solutions.forEach(function(solution) { %>
  				  
											<tr>
												<td><%=solution.problemType %></td>
												<td><%=solution.errorMessage %></td>
												<td><%=solution.desc %></td>
										
											</tr>	
																								
										 <% });} %>
									</tbody>
								</table>
							</div>
						</div>
					</div>
<!-- /.row -->




			</div>
			<!-- /.container-fluid -->
		</div>
		<!-- /#page-wrapper -->
	</div>
	<!-- /#wrapper -->
</body>
<script src="/assets/js/jquery.js"></script>
<script src="/assets/js/bootstrap.min.js"></script>
<script src="/assets/js/jqPaginator.js"></script>
<script src="/assets/js/metisMenu.min.js"></script>
<script src="/assets/js/sb-admin-2.js"></script>
<script type="text/javascript">
	
	
</script>
</html>
